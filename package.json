{
    "name": "divetools2",
    "dependencies": {
        "@shish2k/hyperapp-navigation": "^1.0.0",
        "hyperapp": "^2.0.19",
        "hyperapp-fx": "^2.0.0-beta.2",
        "hyperapp-jsx-pragma": "^1.2.0"
    },
    "scripts": {
        "build": "parcel build --public-url . src/index.html",
        "watch": "parcel watch --public-url . src/index.html",
        "serve": "parcel serve --port 1236 src/index.html"
    },
    "repository": "https://github.com/shish/divetools2/",
    "license": "GPL-3.0-only",
    "pwaManifest": {
        "name": "DiveTools",
        "shortName": "DiveTools",
        "startURL": ".",
        "theme": "#B9FFFF",
        "display": "standalone",
        "orientation": "portrait",
        "generateIconOptions": {
            "baseIcon": "./src/static/favicon.svg",
            "sizes": [
                192,
                384,
                512
            ],
            "genFavicons": true
        }
    },
    "devDependencies": {
        "@parcel/packager-raw-url": "^2.0.0",
        "@parcel/packager-xml": "^2.0.0",
        "@parcel/transformer-sass": "^2.0.0",
        "parcel-config-pwa-manifest": "^0.1.0",
        "parcel": "^2.0.0"
    }
}
