{
    "name": "divetools2",
    "dependencies": {
        "hyperapp": "^2.0.4",
        "hyperapp-auto-history": "^1.0.7",
        "hyperapp-fx": "github:shish/hyperapp-fx#eabef1da8f83ba85af7fda4112be7a11b44e1213"
    },
    "scripts": {
        "build": "parcel build --public-url . src/index.html --experimental-scope-hoisting",
        "watch": "parcel watch --public-url . src/index.html",
        "serve": "parcel serve --port 1236 src/index.html"
    },
    "repository": "https://github.com/shish/divetools2/",
    "license": "GPL-3.0-only",
    "devDependencies": {
        "node-sass": "^4.12.0",
        "parcel-bundler": "^1.12.4",
        "parcel-plugin-pwa-manifest": "^2.9.2",
        "parcel-plugin-sw-cache": "^0.3.1",
        "parcel-plugin-appcache": "^0.1.1",
        "typescript": "^3.8.3"
    },
    "pwaManifest": {
        "name": "DiveTools",
        "shortName": "DiveTools",
        "startURL": ".",
        "theme": "#B9FFFF",
        "display": "standalone",
        "orientation": "portrait",
        "generateIconOptions": {
            "baseIcon": "./src/static/favicon.svg",
            "sizes": [
                192,
                384,
                512
            ],
            "genFavicons": true
        }
    },
    "cache": {
        "disablePlugin": false,
        "inDev": true,
        "strategy": "default",
        "clearDist": true
    }
}
