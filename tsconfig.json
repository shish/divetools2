{
  "compilerOptions": {
    "outDir": "./dist",
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "es6",
    "moduleResolution": "node",
    "target": "es2016",
    "jsx": "react",
    "jsxFactory": "h",
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": true
  },
  "include": ["./src/**/*"]
}